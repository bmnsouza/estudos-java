<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
  version="2.0">

  <named-native-query name="BloqueioResgateModel.buscaPorData" result-class="br.gov.se.sefaz.ndgbackend.model.BloqueioResgateModel">
    <query>
      <![CDATA[
        select * from NFP_ContaCorrente..tb_blr_bloqueio_resgate where cast(blr_dtiniciobloqueio as date) = ?1
      ]]>
    </query>
  </named-native-query>

  <named-native-query name="BloqueioResgateModel.buscaTodos" result-class="br.gov.se.sefaz.ndgbackend.model.BloqueioResgateModel">
    <query>
      <![CDATA[
        select * from NFP_ContaCorrente..tb_blr_bloqueio_resgate order by blr_dtiniciobloqueio
      ]]>
    </query>
  </named-native-query>

  <named-native-query name="BloqueioResgateModel.cadastra">
    <query>
      <![CDATA[
        insert into NFP_ContaCorrente..tb_blr_bloqueio_resgate values (?1, ?2)
      ]]>
    </query>
  </named-native-query>

  <named-native-query name="BloqueioResgateModel.atualiza">
    <query>
      <![CDATA[
        update NFP_ContaCorrente..tb_blr_bloqueio_resgate set blr_msgbloqueio = ?2 where blr_dtiniciobloqueio = ?1
      ]]>
    </query>
  </named-native-query>

  <named-native-query name="BloqueioResgateModel.exclui">
    <query>
      <![CDATA[
        delete from NFP_ContaCorrente..tb_blr_bloqueio_resgate where cast(blr_dtiniciobloqueio as date) = ?1
      ]]>
    </query>
  </named-native-query>

</entity-mappings>