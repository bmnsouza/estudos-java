<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_2_0.xsd"
  version="2.0">

  <named-native-query name="PerfilModel.buscaPorId" result-class="br.gov.se.sefaz.ndgbackend.model.PerfilModel">
    <query>
      <![CDATA[
        select * from USUARIOCAT..TB_PER_PERFIL where per_codPerfil = ?1
      ]]>
    </query>
  </named-native-query>

  <named-native-query name="PerfilModel.buscaTodos" result-class="br.gov.se.sefaz.ndgbackend.model.PerfilModel">
    <query>
      <![CDATA[
        select * from USUARIOCAT..TB_PER_PERFIL order by per_codPerfil
      ]]>
    </query>
  </named-native-query>

</entity-mappings>